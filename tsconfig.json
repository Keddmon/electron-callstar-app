{
    "compilerOptions": {
        "target": "ES2020",
        "module": "CommonJS",
        // "module": "NodeNext",
        "moduleResolution": "Node",
        // "moduleResolution": "NodeNext",
        "rootDir": "src/main",
        "outDir": "dist/main",
        "strict": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "sourceMap": true,
        "types": ["node", "electron"]
    },
    "include": ["src/main/**/*"]
}
